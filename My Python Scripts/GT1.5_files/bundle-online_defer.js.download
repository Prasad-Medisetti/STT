(function ($) {

    'use strict';

    $(document).ready(function () {

        // Init here.
        var $body = $('body'),
            $main = $('#main'),
            $site = $('html, body'),
            transition = 'fade',
            smoothState;

        smoothState = $main.smoothState({
            onBefore: function($anchor, $container) {
                var current = $('[data-viewport]').first().data('viewport'),
                    target = $anchor.data('target');
                current = current ? current : 0;
                target = target ? target : 0;
                if (current === target) {
                    transition = 'fade';
                } else if (current < target) {
                    transition = 'moveright';
                } else {
                    transition = 'moveleft';
                }
            },
            onStart: {
                duration: 400,
                render: function (url, $container) {
                    $main.attr('data-transition', transition);
                    $main.addClass('is-exiting');
                    $site.animate({scrollTop: 0});
                }
            },
            onReady: {
                duration: 0,
                render: function ($container, $newContent) {
                    $container.html($newContent);
                    $container.removeClass('is-exiting');
                }
            },
        }).data('smoothState');

    });

}(jQuery));
var showAmphiSearchSelectBox;
var loadDoubtThreadStatus = true;
var doubtThreadMap = {};
var selectedMentorIdList = new Array(), ScheduleListNew = [];
var notificationSelected= 2;
var loadedSubmissionId = -1, loadedSubmissionOffset = 0;
var loadedDoubtThreadOffset = 0 , isDoubtThreadLoading = false, isDoubtThreadCompletlyLoaded = false;
var skillWiseCountShown = false, liveParticipantCount = 0;
var isMessageLoading = false, isMessageCompletelyLoaded = false, messageOffsetOfDoubtThread = 0;
var nextIdSearch = false;
var searchMode = false, isDropDownHovered = false;
function showLoading() {
    $("#cmProcessDialogProcessing").modal('hide');
    $("#cmProcessDialog").modal('show');
    $("#cmProcessDialog").addClass('show');
}

function hideLoading() {
    $("#cmProcessDialog").modal('hide');
    $("#cmProcessDialog").removeClass('show');
    $('.modal-backdrop.fade-in').css("display","none");
}

function loadSubmissions(element, asuaId, componentId, problemId){
    var contextPathURL = contextPath;
    showLoading();
    $.ajax({
        method:"post",
        url:contextPathURL+"/doubtThread/obtainSubmissionsForUserByProblem",
        data : { asuaId : asuaId, componentId : componentId, problemId : problemId},
        success : function(data){
            $('#doubt-message-attempt-container').html(data);
            loadSyntaxHighLighter();
            chatpage_HeightCalculate();
            $('.submission-thread').removeClass('active');
            $(element).addClass('active');
        }
    }).done(function(){ hideLoading();});
}

function loadDoubtThreads(showLoader){
    var contextPathURL = contextPath;
    var map = { selectAssigned : "off", selectClosed : "off", selectUnAssigned : "off", selectSLAFailed : "off", selectProactiveSupport : "off", limitByDate : 'off', dayLimit :'-1', startFrom : 0, sortById : "off", sortingOrder : "desc", offset : loadedDoubtThreadOffset};
        map['selectedCourse'] = $("select[name='selectedCourse']").val();
        map['selectedSkill'] = $("select[name='selectedSkill']").val();
        map['selectedInstitution'] = $("select[name='selectedInstitution']").val();
        map['selectedTeam'] = $("select[name='selectedTeam']").val();
    $('.minidropdown li.active').each(function(){
        var key = $(this).attr("data");
        if(key === "limitByDate"){
            map['dayLimit'] =  $(this).attr("date-limit");
            map['startFrom'] =  $(this).attr("start-from");
        }else if(key === "sortById"){
            map['sortingOrder'] =  ($(this).attr("order") === "asc" ? 'asc' : 'desc');            
        }
        map[key] = "on";
    });
    if(!isDoubtThreadLoading && !isDoubtThreadCompletlyLoaded){
        isDoubtThreadLoading = true;
        $('#small-spinner').jmspinner();
        $.ajax({
            method:"POST",
            url: contextPathURL+"/doubtThread/obtainDoubtThreads",
            data: map,
            async:false,
            success:function(data){
                if(data !== "")
                    doubtThreadJsonToHtml(JSON.parse(data),false);
                else{
                    $('.eboxchat ul').append("<li class='doubt-thread' style='text-align: center;cursor: inherit;padding:15px;'>No more Doubts</li>");
                }
            },
            error: function() {
              console.log('Error!');
            }
        }).done(function(){ isDoubtThreadLoading = false;$('#small-spinner').jmspinner(false);});
    }
}

function loadMoreSubmissions(offset){
    loadedSubmissionOffset += offset;
    loadRecentSubmissions();
}

function loadRecentSubmissions(showLoader){
    var contextPathURL = contextPath;
    var map = { selectProjectSubmissions : "off", selectContestSubmissions : "off", selectDbmsQuerySubmissions : "off", offset : loadedSubmissionOffset, max : 50,
                sortByMoreRetry : "off", sortByMorePassPercentage : "off", limitByDate : 'off', dayLimit :'-1', 'startFrom' : 0};
        map['selectedCourse'] = $("select[name='selectedCourse']").val();
        map['selectedSkill'] = $("select[name='selectedSkill']").val();
        map['selectedInstitution'] = $("select[name='selectedInstitution']").val();
        map['selectedTeam'] = $("select[name='selectedTeam']").val();
    $('.minidropdown li.active').each(function(){
        var key = $(this).attr("data");
        if(key === "limitByDate"){
            map['dayLimit'] =  $(this).attr("date-limit");
            map['startFrom'] =  $(this).attr("start-from");
        }
        map['sortingOrder'] =  ($(this).attr("order") === "asc" ? 'asc' : 'desc');
        map[key] = "on";
    });
    $('#small-spinner').jmspinner();
    $.ajax({
        method:"POST",
        url: contextPathURL+"/doubtThread/loadRecentSubmissions",
        data: map,
        success:function(data){
            $('.eboxchat').html(data);
            if(showLoader){refreshMainPage();}
            $('.eboxchat').scrollTop(0);
        },
        error: function() {
          console.log('Error!');
        }
    }).done(function(){ $('#small-spinner').jmspinner(false); });    
}

function refreshMainPage(){
    $('#doubt-message-attempt-container').html("<i class='doubt-center-image'></i>");
}

function markDoubtThreadStatus(threadId, status, reloadDoubtThreadTab){
    var contextPathURL = contextPath;
    $.ajax({
        method:"post",
        async:false,
        url: contextPathURL+"/doubtThread/markDoubtThreadStatus",
        data:{'doubtThreadId':threadId, 'status': status},
        success : function(data){
            if(reloadDoubtThreadTab){
                obtainDoubtThreadMessages(threadId)
            }
            $('#doubt-help-status-'+threadId).html(status);
            var color = status === "Re-opened" ? "#c60000" : status === "Opened" ? "#ffa511":"#888888";
            $('#doubt-help-status-'+threadId).css('background-color',color);
        }
    });    
}

function loadActiveTabSection(){
    if($('.section-tab.active').length === 0)
        loadSectionTabByContent("doubts");
    else
        loadSectionTabByContent($('.section-tab.active').attr("tabContent"));    
}

function loadSubmissionTab(){
    $(".limiting-filter li[data='limitByDate']").removeClass("active");
    $(".limiting-filter li[date-limit='3']").addClass("active");
    loadSectionTabByContent("submissions");
}

function loadSectionTabByContent(tabContent){
    loadedDoubtThreadOffset = 0;
    loadedSubmissionOffset = 0;
    isDoubtThreadCompletlyLoaded = false;
    $(".eboxchat ul").html("");
    $('#doubt-message-attempt-container').html("");
    $('.section-tab').removeClass("active");
    $(".section-tab[tabcontent='doubts']").html("DOUBTS");
    $(".section-tab[tabContent='"+tabContent+"']").addClass("active");
    $('.togglebar').hide();
    if(tabContent === "doubts"){
        loadDoubtThreads(true);
        $('.doubt-thread-id-filter-class').show();
        $('.doubt-thread-filter').show();
        $('.submission-filter').hide();
    }
    else if(tabContent === "submissions"){
        loadRecentSubmissions(true);
        $('.doubt-thread-id-filter-class').hide();
        $('.doubt-thread-filter').hide();
        $('.submission-filter').show();
    }
};

function loadSectionTabByContentForSME(tabContent){
    $('.supportmenus ul li').removeClass("active");
    $('.tutoring-dashboard').html("");
    $('.tutoring-plan-dashboard').html("");
    $("#main-detail-area").hide();
    if(tabContent === "dashboard"){
        $('#pearl').hide();
        $('.section-sme-doubt-submission').hide();
        $('.section-sme-dashboard').show();
        $(".doubt-thread-filter[data='selectSLAFailed']").removeClass("active");
        $(".doubt-thread-filter[data='selectProactiveSupport']").removeClass("active");
    }
    else{
        $('.section-sme-dashboard').hide();
        loadSectionTabByContent(tabContent);
        $('#pearl').show();
        $('.section-sme-doubt-submission').show();
        chatpage_HeightCalculate();
    }
    $(".supportmenus ul li[data-content='"+tabContent+"']").addClass("active");
};

function chatpage_HeightCalculate(){
    $(".chatlists, .othercontent, .messagecontent").height($(window).outerHeight() - 50);
    $("#scrollable-message-wrapper").height($(window).height() - 140);
    $(".eboxchat").height($(window).height() - (160-($('.doubtssection').length > 0 ? 0 :50)));
};

function triggerMessage(event){
    if (event.keyCode === 13 && (event.shiftKey || event.ctrlKey)) {
        var doubtId = $('#assignToMentorDoubtThreadId').val();
        addDoubtMessage(doubtId);
    }
    else{
        var element = document.getElementById("textdf");
        element.style.height = "10px";
        element.style.height = (element.scrollHeight+15)+"px";
    }
};

function obtainDoubtThreadMessages(id){
    hideDropDownDetailsData(0);
    var element = $(".doubt-thread[data-id='"+id+"']");
    var contextPathURL = contextPath;
    showLoading();
    $.ajax({
        method:"post",
        url: contextPathURL+"/doubtThread/obtainMessagesForDoubt",
        data : { 'doubtThreadId' : id, max:20, offset : 0},
        success : function(data){
            showMessageBox();
            loadedDoubtThreadId = id;
            messageOffsetOfDoubtThread = 0;
            isMessageCompletelyLoaded = false;
            $('#doubt-message-attempt-container').html(data);
            chatpage_HeightCalculate();
            $('.doubt-thread').removeClass('active');
            $(element).addClass('active');
            loadSyntaxHighLighter();
        }
    }).done(function(){ hideLoading();});
}

function loadDoubtMessagesAsJSON(doubtThreadId){
    var contextPathURL = contextPath;
    var dateNotification = "", addedDateList = new Array();
    $('.page-down-loader').show();
    isMessageLoading = true;
    $.ajax({
        method:"post",
        url:contextPathURL+"/doubtThread/obtainMessagesAsJSON",
        async:false,
        data:{doubtThreadId:doubtThreadId, max:20, offset : messageOffsetOfDoubtThread},
        success:function(data){
            if(data.length === 0){
                isMessageCompletelyLoaded = true;
                $('#scrollable-message-wrapper').prepend("<div class='notification'><span class='message-notification'>Start of doubt conversation</span></div>");
            }
            else{
                messageOffsetOfDoubtThread += data.length;
                for(i =0;i < data.length;i++){
                    if(data[i].messageType === "chat"){
                        data[i].dateOfMessage = (data[i].dateOfMessage.endsWith("am") || data[i].dateOfMessage.endsWith("pm")) ? 'Today' : data[i].dateOfMessage
                        if(dateNotification !== "" && dateNotification !== data[i].dateOfMessage && addedDateList.indexOf(dateNotification) === -1){
                            $('#scrollable-message-wrapper').prepend("<div class='notification'><span class='message-notification'>"+dateNotification+"</span></div>");
                            addedDateList.push(dateNotification);
                        }
                        dateNotification = data[i].dateOfMessage;
                        var html  = "<div class='chatcover'><div class='"+(data[i].isPosted === true ? 'chat2':'chat1')+"'>";
                            if(data[i].message.indexOf("\n") > 0){
                                html += "<span class='fa fa-window-maximize code-align' onclick=\"jQuery(this).toggleClass('fa-window-maximize');jQuery(this).toggleClass('fa-window-minimize');jQuery(this).next().toggleClass('display-code');\"></span>";
                            }
                            html += "<pre id='doubt-message-id'>"+data[i].message+"</pre>";
                            html += "<span class='time'><span>"+data[i].postedUserName+"</span> ["+data[i].repliedTime+"]"+(data[i].isPosted === true ? "<img alt='.' title='.' src='"+contextPathURL+"/static/images/online/icons/"+data[i].status+".png' />":"")+"</span>";
                            html += "</div></div>";
                        $('#scrollable-message-wrapper').prepend(html);
                        $('#doubt-message-id').text(data[i].message+"");
                        $('#doubt-message-id').removeAttr("id");
                    }else{
                        $('#scrollable-message-wrapper').prepend("<div class='notification'><span class='message-notification'>"+data[i].message+"</span></div>");
                    }
                }
            }
            isMessageLoading = false;
        }
    }).done(function(){ isMessageLoading = false; $('.page-down-loader').hide();});;
}    

function doubtThreadMoreDetails(obj){
    var html = [
            '<div id="dropdown-details-data-'+obj.key+'" style="display:none;">',
                '<div class="form-group">',
                    '<span class="title">Participant</span>',
                    '<span class="content">'+obj.participantUsername+'</span>',
                    '<span class="copy-text" title="Copy to clipboard" onclick="copyTextToClipBoard(\''+obj.participantUsername+'\')"></span>',
                '</div>',
                '<div class="form-group">',
                    '<span class="title">Mentor</span>',
                    '<span class="content">'+obj.mentorUsername+'</span>',
                    '<span class="copy-text" title="Copy to clipboard" onclick="copyTextToClipBoard(\''+obj.mentorUsername+'\')"></span>',
            ].join('\n');
        if(obj.mentorEmail!=''){
             html+=[
                '</br>',
                '<span class="title"></span>',
                '<span class="content" style="font-size:11px;">'+obj.mentorEmail+'</span>',
                '<span class="copy-text" title="Copy to clipboard" onclick="copyTextToClipBoard(\''+obj.mentorEmail+'\')"></span>',
            ].join('\n');
        }
        html+=[
            '</div>',
            '</div>'].join('\n');
    return html;
}

function doubtThreadJsonToHtml(json,toPrepend){
    var contextPathURL = contextPath;
    var rowCount = 0;
    var dataId = -1;
    if($('.doubt-thread.active').length > 0)
        dataId = Number($('.doubt-thread.active').attr("data-id"));
    $.each(json, function(i, obj) {
        var html = "";
        rowCount++;
        var keyId = obj.key+"";
        doubtThreadMap[keyId] = keyId;
        if($('#doubt-thread-'+keyId).length > 0){
           $('#doubt-thread-'+keyId).remove();
        }
        html += "<li class='doubt-thread' "+($("#selectedUsername").length > 0 ? "onmouseenter='showDropDownDetailsData(this,event)'":"")+" onclick = 'obtainDoubtThreadMessages("+keyId+")' id='doubt-thread-"+keyId+"' data-id = '"+keyId+"'>";
        if(''+(obj.isOnline) === "true")
            html += "<div id='notreadicon-"+keyId+"' class='notreadIcon'></div>";
        html += "<div class='onlineavatar' style='background-image:url("+(obj.hasPic ? contextPathURL+"/user/showProfilePicture/"+obj.displayUserId :contextPathURL+"/static/images/online/default-user-connect-live.png")+")'></div>";
        html += "<div class='doubt-thread-id-holder'><span>#"+keyId+"</span></div>";
        html += "<div class='centerchat'>";
        html += "<label id='doubt-thread-msg-content-"+keyId+"'></label>";
        html += "<p title='"+obj.notificatedUserName+"'>"+(obj.displayUserName === undefined ? 'Un Assigned' :obj.displayUserName)+"</p>";
        html += "<div class='clr'></div>";
        if(obj.skillName !== undefined && obj.skillName.length > 0)
            html += "<div class='colorNotification violetcolor'>"+obj.skillName+"</div>";
        if(obj.helpStatus !== undefined && obj.helpStatus.length > 0){
            var color = obj.helpStatus === "Re-opened" ? "#c60000" : obj.helpStatus === "Opened" ? "#ffa511":"#888888"; 
            html += "<div class='colorNotification' id='doubt-help-status-"+keyId+"' style='background-color:"+color+";'>"+obj.helpStatus+"</div>";
        }
        html += "<div class='doubt-progress' style='width:auto;background-color:inherit;margin-top: 10px;margin-bottom:0px;'>";
        html += "<span class='"+(obj.blinkStatus ? 'blink_text':'')+"' style='color:"+obj.colorIndication+"'>"+obj.status+"</span>";
        html += "</div>";
        if(obj.institutionName !== undefined && obj.institutionName !== ""){
            html +="<div class='clr'></div><div title='"+obj.institutionName+"'class='dbt-institution-name'>"+obj.institutionName+"</div>"+doubtThreadMoreDetails(obj)+"";
        }
        html += "</div>";
        html += "<div class='chatright'>";
        if(obj.unReadMessageCount > 0)
            html += "<label id='doubt-unread-msg-count-"+keyId+"' class='notificationlbl'>"+obj.unReadMessageCount+"</label>";
        html += "<div class='clr'></div>";
        html += "</div>";
        html += "<div class='clr'></div>";
        html += "</li>";
        if($(".doubt-thread[data-id='"+keyId+"']").length > 0){
           $(".doubt-thread[data-id='"+keyId+"']").remove(); 
        }
        if(toPrepend === false)
            $('.eboxchat ul').append(html);
        else
            $('.eboxchat ul').prepend(html);
        $('#doubt-thread-msg-content-'+keyId+'').text(obj.message);
        if(dataId > 0){
            $(".doubt-thread[data-id='"+dataId+"']").addClass("active");
            document.getElementById("doubt-thread-"+dataId).scrollIntoView();
            document.getElementById('eboxchat').scrollTop -= 120;
            if($('#doubt-thread-'+keyId).hasClass('active')){
                addMessageToWrapper(obj.message,'chat1');
            }
        }
    });
    if(rowCount > 0){
        loadedDoubtThreadOffset +=rowCount;
        //$(".section-tab[tabContent='doubts']").html("DOUBTS<span>"+""+"</span>");
    }
    else{
        isDoubtThreadCompletlyLoaded = true;
        if(!toPrepend ){
            $('.eboxchat ul').append("<li class='doubt-thread' style='text-align: center;cursor: inherit;padding:15px;'>No more Doubts</li>");
        }
    }
    if(toPrepend){
        addNotificationCountToPanel(rowCount);
    }
}

function addMessageToWrapper(message,className){
    var date = new Date();
    var hours = date.getHours();
    var minutes = date.getMinutes();
    var ampm = hours >= 12 ? 'pm' : 'am';
    hours = hours % 12;
    hours = hours ? hours : 12;
