function amphiWarningDialog(message,yesAction,noAction){
        amphiConfirmStatus = false;
        actionCalled = false;
        $("#amphiWarningDialogDiv").html(message);
        $("#amphiWarningDialogModal").modal('show');
        $("#amphiWarningDialogYes").click(function(){
            amphiConfirmStatus = true;
            if(!actionCalled){
                actionCalled = true;
                yesAction();    
                $("#amphiWarningDialogYes").unbind('click');
                $("#amphiWarningDialogModal").unbind('hide.bs.modal');
            }
        });
        $("#amphiWarningDialogModal").on("hide.bs.modal",function(e){
            if(!amphiConfirmStatus){
                if(!actionCalled){
                    actionCalled = true;
                    noAction();
                    $("#amphiWarningDialogModal").unbind('hide.bs.modal'); 
                    $("#amphiConfirmDialogYes").unbind('click');
                }
            }
        });
    }
