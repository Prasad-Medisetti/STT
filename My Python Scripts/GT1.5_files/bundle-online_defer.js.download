function toggleVideo(ele){            
    if($(ele).attr('status')=='on'){
        $(ele).attr('status','off');
        webrtc.pauseVideo();
        $(ele).removeClass("btn-success");
        $(ele).addClass("btn-danger");
    }else{
        $(ele).attr('status','on');
        webrtc.resumeVideo()
        $(ele).addClass("btn-success");
        $(ele).removeClass("btn-danger");
    }
}

