function checkUserDetails(role,priority,sessionIns,userIns) {
    $(".errorProfile").hide();
    var re = /^(([^<>()\[\]\\.,;:\s@"]+(\.[^<>()\[\]\\.,;:\s@"]+)*)|(".+"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;
    var name = $("#name").val().trim();
    var dob = $("#dob").val().trim();
    $("#name").val(name);
    $("#dob").val(dob);
    var pattern = new RegExp(/\b\d{1,2}[\-]\d{1,2}[\-]\d{4}\b/); 
    var flag = true;
    if(name == "" || dob == "") {
        $("#errorUpdateProfile").html("Please enter all the fields");
        $("#errorUpdateProfile").show();
        flag = false;
    }
    else if(!(pattern.test(dob))){
        $("#errorUpdateProfile").html("Please enter valid date");
        $("#errorUpdateProfile").show();
        flag = false;
    }
    if(role < priority && sessionIns == userIns){
        var registerNumber = $("#registerNumber").val().trim();
        $("#registerNumber").val(registerNumber);
        if(registerNumber == ""){
            $("#errorUpdateProfile").html("Please enter all the fields");
            $("#errorUpdateProfile").show();
            flag = false;
        }
    }
    else if(role < priority && sessionIns!=userIns){
        var email = $("#email").val().trim();
        var registerNumber = $("#registerNumber").val().trim();
        var mobilenumber = $("#mobilenumber").val().trim();
        $("#email").val(email);
        $("#registerNumber").val(registerNumber);
        $("#mobilenumber").val(mobilenumber);
        if(email == "" || registerNumber == "" || mobilenumber == ""){
            $("#errorUpdateProfile").html("Please enter all the fields");
            $("#errorUpdateProfile").show();
            flag = false;
        }
        if(!re.test(email)){
            $("#errorUpdateProfile").html("Enter valid mail id");
            $("#errorUpdateProfile").show();
            flag = false;
        }
    }
    return flag;
}

