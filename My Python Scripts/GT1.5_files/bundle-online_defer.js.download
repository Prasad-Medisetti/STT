function loadDoubtsForDoubtThread(doubtThreadId,clusterPattern,problemId,problemType){
    $("#selected-msg").empty();
    $('#messages').empty('');
    $('#valid-message').hide();
    $('#edit-message').hide();
    var contextPathURL = contextPath;
    var dateNotification = "", addedDateList = new Array();
    $('.page-down-loader').show();
    isMessageLoading = true;
    $.ajax({
        url: contextPathURL+"/doubtThread/obtainValidResponseForProb",
        data:{clusterPattern:clusterPattern,problemId:problemId,problemType:problemType},
        success: function(data) {
            $('.valid-auto-reply-msg').html('');
            if(data.length===0) {
                $('.valid-auto-reply-msg').append('<p style="text-align: center;">No Valid Auto Replies.</p>');
            } else {
                for (i = 0; i < data.length; i++) {
                    var html = "<div class='chat2' style='background: #d0f1ff;'>";
                    html += "<pre id='doubt-message-id'"+i+">" + data[i].message.escapeHTML() + "</pre>";
                    if (data[i].message.indexOf("\n", 1) != -1) {
                        html+="<span class='fa fa-window-maximize code-align' onclick=\"jQuery(this).toggleClass('fa-window-maximize');jQuery(this).toggleClass('fa-window-minimize');jQuery(this).prev().toggleClass('display-code');\"></span>";
                    }
                    html += "</div>";
                    $('#valid-auto-reply-msg').prepend(html);
                }
                $('#valid-auto-reply-msg').prepend("<h3 class='modal-title' id='lineModalLabel' style='margin-left: 108px;'>Valid Auto Reply Messages</h3>")
            }
            
        }
    });
    $.ajax({
        method:"post",
        url:contextPathURL+"/doubtThread/obtainMessagesAsJSON",
        async:false,
        data:{doubtThreadId:doubtThreadId, max:20, offset : messageOffsetOfDoubtThread},
        success:function(data){
            if(data.length === 0){
                isMessageCompletelyLoaded = true;
                $('#scrollable-message-wrapper').prepend("<div class='notification'><span class='message-notification'>Start of doubt conversation</span></div>");
            }
            else{
                messageOffsetOfDoubtThread += data.length;
                for(i =0;i < data.length;i++){
                    if(data[i].messageType === "chat"){
                        data[i].dateOfMessage = (data[i].dateOfMessage.endsWith("am") || data[i].dateOfMessage.endsWith("pm")) ? 'Today' : data[i].dateOfMessage
                        if(dateNotification !== "" && dateNotification !== data[i].dateOfMessage && addedDateList.indexOf(dateNotification) === -1){
                            $('#scrollable-message-wrapper').prepend("<div class='notification'><span class='message-notification'>"+dateNotification+"</span></div>");
                            addedDateList.push(dateNotification);
                        }
                        dateNotification = data[i].dateOfMessage;
                        var html  = "<div class='chatcover'><div class='"+(data[i].isPosted === true ? 'chat2 valid-msg':'chat1 sender-msg')+"'>";
                        if(data[i].isPosted) {
                                html +="<div class='code-align select-message'><label><input type='checkbox' class='valid-msg-selection checkbox1' id='msg"+i+"' onclick=\"(markMessageAsValid('" + data[i].message.replace(/\r?\n|\r/g, '\\n').replace(/'/g, "\\'").replace( /"/g, '&quot;' ) + " ','" + i + " '))\" name='selectedCatalog' value='1'><label for='msg"+i+"'  style='margin-top: 0px;margin-right: 0px;' title='Mark as Valid Message'></label></label></div>"
                            }
                            if(data[i].message.indexOf("\n") > 0){
                                html += "<span class='fa fa-window-maximize code-align' onclick=\"jQuery(this).toggleClass('fa-window-maximize');jQuery(this).toggleClass('fa-window-minimize');jQuery(this).next().toggleClass('display-code');\"></span>";
                            }
                            html += "<pre id='doubt-message-id'>"+data[i].message+"</pre>";
                            html += "<span class='time'><span>"+data[i].postedUserName+"</span> ["+data[i].repliedTime+"]"+(data[i].isPosted === true ? "<img alt='.' title='.' src='"+contextPathURL+"/static/images/online/icons/"+data[i].status+".png' />":"")+"</span>";
                            html += "</div></div>";
                        $('#scrollable-message-wrapper').prepend(html);
                        $('#doubt-message-id').text(data[i].message+"");
                        $('#doubt-message-id').removeAttr("id");
                    }else{
                        $('#scrollable-message-wrapper').prepend("<div class='notification'><span class='message-notification'>"+data[i].message+"</span></div>");
                    }
                }
                $('#hidden-fields').html("<input type='hidden' value="+clusterPattern+" name='clusterPattern'><input type='hidden' value="+problemId+" name='problemId'><input type='hidden' value="+problemType+" name='problemType'>")
                $('#scrollable-message-wrapper').prepend("<h3 class='modal-title' id='lineModalLabel'>Conversations</h3>");
            }
            isMessageLoading = false;
        }
    }).done(function(){ isMessageLoading = false; $('.page-down-loader').hide();});;
}    

