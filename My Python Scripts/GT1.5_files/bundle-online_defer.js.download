function treeViewAndContextMenu(){
    //tree view and context menu
    $(".amphiTreeView").each(function(){
            var url = $(this).attr("items-url");
            var query=null;
            var $this=$(this);
            if($this.attr('query')!=undefined && $this.attr('query')!=null && $this.attr('query')!="" ){
                query=eval("("+$(this).attr('query')+")");
            }
            var rootAttributes = null;
            if($this.attr('rootAttributes')!=undefined && $this.attr('rootAttributes')!=null && $this.attr('rootAttributes')!="" ){
                rootAttributes=eval("("+$(this).attr('rootAttributes')+")");
            }
            $.ajax({
                url:url,
                type: "POST",
                dataType:'text',
                success : function(data){
                    data=eval("("+data+")");
                    var ul=$("<ul />");
                    ul.addClass("tree-view filetree");
                    var span=$("<span />").html($this.attr('rootLabel')).attr("id",$this.attr('rootId'));
                    if(data.items[0]!=undefined && data.items[0].label!=undefined){
                        span.append(" - ",data.items[0].label.text);
                    }
                    span.attr("grandParent",1);
                    if(rootAttributes){
                        for(var attribute in rootAttributes){
                            span.attr(attribute,rootAttributes[attribute]);
                        }                        
                    }
                    if(span.attr("title") == undefined && span.attr("name") != undefined){
                        span.attr("title",span.attr("name"))
                    }
                    span.addClass("tree-item");
                    if(data.items.length>1){
                      span.addClass("folder");
                    }else if(data.items.length>0 && data.items[0].id!=$this.attr('rootId')){
                      span.addClass("folder");
                    } else{
                      span.addClass("file");
                    }
                    ul.append($("<li />").append(span).append(getTree(data.items,query)));
                    $this.append(ul);
                    $(".tree-view").treeview();
                    $.each($(".tree-view"),function(i,val){
                          if($(val).is(':hidden')){
                                $(val).find("span.folder").click();
                          }
                    });
                    if($this.attr('itemClick')!=undefined && $this.attr('itemClick')!=null && $this.attr('itemClick')!="" ){
                        $(".tree-item").mousedown(function(event){
                            $(".tree-view").find(".tree-item.clicked").removeClass("clicked");
                            $(this).addClass("clicked");
                            eval("("+$this.attr('itemClick')+")");
                        });
                        $(".tree-item").click(function(event){
                            if(event.button != 2){
                                eval("("+$this.attr('itemLeftClick')+")");
                            }
                        });
                    }
                    if($this.attr('selectedItem')!=undefined && $this.attr('selectedItem')!=null && $this.attr('selectedItem')!="" ){
                        $(".tree-view").find("span[id="+$this.attr('selectedItem')+"]").mousedown();
                    }
                    if($this.attr('oncompleted')!=undefined && $this.attr('oncompleted')!=null && $this.attr('oncompleted')!=""){
                        if(jQuery.isFunction(window[$this.attr('oncompleted')])){
                            window[$this.attr('oncompleted')](data);
                        }
                    }
                    
                },
                error: function (xhr, ajaxOptions, thrownError) {
                    //console.log(xhr.status);
                    //console.log(thrownError);
                }
            });
          });
         var $contextMenu=$("#tree_menu");
         $("body").on("contextmenu", ".tree-item", function(e) {
            $contextMenu.css({
              display: "block",
              left: e.pageX,
              top: e.pageY
            });
            return false;
          });
          $(document).mousedown(function(){
            $contextMenu.hide();
          });
}

