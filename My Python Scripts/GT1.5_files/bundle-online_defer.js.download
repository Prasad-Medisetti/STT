function dragCircuitResizer(){
    var leftWidth, RightWidth, initialPostionLeft, positionleft;
    $('#resizer').draggable({
        axis: "x",
        containment: 'parent',
        start:function(event, ui){
            initialPostionLeft = ui.position.left;
            leftWidth = $('#left').width();
            RightWidth = $('#right').width();
        },
        drag:function(event, ui){
            positionleft = ui.position.left;
            var leftSetWidth = leftWidth + positionleft - initialPostionLeft;
            var rightSetWidth = RightWidth - positionleft + initialPostionLeft;
            if(leftSetWidth < 250 || rightSetWidth < 550 ){
                return false;
            } else {
                $("#left").width(leftWidth + positionleft - initialPostionLeft);
                $("#right").width(RightWidth - positionleft + initialPostionLeft);
            }
            editorBtnActionStyle(RightWidth - positionleft + initialPostionLeft);
            var th = headSectionHeight();
            var bh = bottomPostHeight();
            var tbar = $('#title-bar').height();
            if(document.querySelector('.program-wrapper #editor-scroll')){setScrollHeight(window.innerHeight, th+bh+tbar)};
            
        }
    });
}
