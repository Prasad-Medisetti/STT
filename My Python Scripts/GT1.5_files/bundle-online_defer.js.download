var lineChartAnimatedFusion = function (option) {

    console.log(option)
    var type = option.type; // On Hold, to be done after integration
    var id = option.id;
    var inputWidth = isNaN(option.width)||typeof option.width === "undefined" ? parseInt(d3.select("#total-wrapper").style("width"))-20 : option.width;
    var inputHeight = isNaN(option.height)||typeof option.height === "undefined" ? 150 : option.height;
    var targetId = option.renderAt;
    var dataSource = option.dataSource;
    // Manual addition of attributes
    var showLegend = typeof option.showLegend === "undefined" ? false: option.showLegend;
    var title = typeof option.title === "undefined" ? false: option.title;
    var xAxisLabel = typeof option.xAxisLabel === "undefined" ? false: option.xAxisLabel;
    var yAxisLabel = typeof option.yAxisLabel === "undefined" ? false: option.yAxisLabel;
    var xAxisIsTime = typeof option.xAxisIsTime === "undefined" ? false: option.xAxisIsTime;
    var margin = typeof option.margin === "undefined" ? { top: 0, right: 50, bottom: 0, left: 0 }: option.margin;
    margin.top = typeof option.marginTop === "undefined" ? margin.top: option.marginTop;
    margin.right = typeof option.marginRight === "undefined" ? margin.right: option.marginRight;
    margin.bottom = typeof option.marginBottom === "undefined" ? margin.bottom: option.marginBottom;
    margin.left = typeof option.marginLeft === "undefined" ? margin.left: option.marginLeft;
    var color = typeof option.color === "undefined" ? "#FF8711": option.color;
    var circleColor = typeof option.circleColor === "undefined" ? "#ff0000": option.circleColor;
    var titleFontSize = typeof option.titleFontSize === "undefined" ? "6px": option.titleFontSize;
    var xAxisLabelFontSize = typeof option.xAxisLabelFontSize === "undefined" ? "11px": option.xAxisLabelFontSize;
    var yAxisLabelFontSize = typeof option.yAxisLabelFontSize === "undefined" ? "11px": option.yAxisLabelFontSize;
    var xGridLines = typeof option.xGridLines === "undefined" ? true: option.xGridLines;
    var yGridLines = typeof option.yGridLines === "undefined" ? true: option.yGridLines;
    var types = typeof option.types === "undefined" ? false: option.types;
    var xAxis = typeof option.xAxis === "undefined" ? true: option.xAxis;
    var yAxis = typeof option.yAxis === "undefined" ? true: option.yAxis;
    var calloutLink = typeof option.calloutLink === "undefined" ? false: option.calloutLink;
    var calloutInvLink = typeof option.calloutInvLink === "undefined" ? false: option.calloutInvLink;
    var startTime = typeof option.startTime === "undefined" ? null: option.startTime;
    var endTime = typeof option.endTime === "undefined" ? null: option.endTime;

    var intTitleFontSize = parseInt(titleFontSize.replace("px",""));
    var intXAxisLabelFontSize = parseInt(xAxisLabelFontSize.replace("px",""));
    var intYAxisLabelFontSize = parseInt(yAxisLabelFontSize.replace("px",""));
    if((inputHeight<200 && inputHeight>100)&& intTitleFontSize>13){
        intTitleFontSize = 14;
    }else if(inputHeight<200 && intTitleFontSize>10){
        intTitleFontSize = 12;
    }
    if((inputHeight<200 && inputHeight>100)&& intXAxisLabelFontSize>11){
        intXAxisLabelFontSize = 15;
    }else if(inputHeight<200 && intXAxisLabelFontSize>8){
        intXAxisLabelFontSize = 8;
    }
    if((inputWidth<200 && inputHeight>100)&& intYAxisLabelFontSize>11){
        intYAxisLabelFontSize = 15;
    }else if(inputWidth<200 && intYAxisLabelFontSize>8){
        intYAxisLabelFontSize = 8;
    }
    titleFontSize = intTitleFontSize + "px";
    xAxisLabelFontSize = intXAxisLabelFontSize + "px";
    yAxisLabelFontSize = intYAxisLabelFontSize + "px";


    if(intTitleFontSize>margin.top){
        margin.top = intTitleFontSize+2;
    }
    if(intXAxisLabelFontSize>margin.bottom){
        margin.bottom = intXAxisLabelFontSize+2;
    }
    if(intYAxisLabelFontSize>margin.left){
        margin.left = intYAxisLabelFontSize+2;
    }



    var data;
    var fixedData
    var animateData
    if (typeof dataSource === "object") {
        dataSource.forEach(function(d){
            // console.log("dataSource d")
            // console.log(d)
            fixedData = typeof d.data.fixedData === "undefined" ? null: d.data.fixedData;
            animateData = typeof d.data.animateData === "undefined" ? null: d.data.animateData;

            // console.log("fixedData and animateData")
            // console.log(fixedData)
            // console.log(fixedData)

            if(fixedData!==null && animateData!==null){
                // console.log("inside if")
                if(fixedData.length>0 && animateData.length>0){
                  fixedDataLastObj = fixedData[fixedData.length -1];
                  animateDataFirstObj = animateData[0];
                  if(fixedDataLastObj["label"]!==animateDataFirstObj["label"] || fixedDataLastObj["value"]!==animateDataFirstObj["value"] || fixedDataLastObj["type"]!==animateDataFirstObj["type"]){
                    animateData = [{"label":fixedDataLastObj["label"],"value":fixedDataLastObj["value"],"type":fixedDataLastObj["type"]}].concat(animateData);
                  }
                  // console.log(animateData)
                }
            }
            d.data.animateData = animateData
        })
    } else if (typeof dataSource === "string") {

        $.ajax({
            url: dataSource,
            success: function (result) {
                data = result;
            }
        });
    }


//    var defaultColorArray = ["#ef5350", "#EC407A", "#AB47BC", "#7E57C2", "#5C6BC0", "#42A5F5", "#29B6F6", "#26C6DA", "#26A69A", "#66BB6A", "#9CCC65", "#D4E157", "#FFEE58", "#FFCA28"];


    // Linechart core

    //$("#"+id).remove();
    var svg = d3.select("#"+targetId).append("svg").attr("id",id).attr("width",inputWidth).attr("height",inputHeight),
        g = svg.append("g").attr("transform", "translate(" + 0+ "," +18+ ")");

    // set the dimensions of the graph
    var width = inputWidth - margin.left - margin.right;
    var height = inputHeight - margin.top - margin.bottom;

    // Drawing Labels
    // Drawing chart title
    if(title!==false){
        svg.append("text")
                .attr("x",width-200)
                .attr("y","13")
                .attr("font-size",titleFontSize)
                .attr("fill","#ff3385")
                .attr("font-weight",20)
                .text(pairUserName+" : "+title);
    }
    if(xAxisLabel!==false){
        svg.append("text")
                .attr("x", width/2)
                .attr("y", height + margin.top + margin.bottom)
                .attr("font-size",xAxisLabelFontSize)
                .attr("fill","black")
                .text(xAxisLabel);
    }
    if(yAxisLabel!==false){
        svg.append("text")
                .attr("x",0)
                .attr("y", margin.top + height/2 )
                .attr("font-size",yAxisLabelFontSize)
                .attr("fill","black")
                .attr("transform","rotate(-90,"+margin.left/3+","+(0+parseInt(margin.top) + parseInt(height)/2)+")")
                .text(yAxisLabel);
    }

    //  Datacopy construction
    var timeParse = d3.timeParse("%d/%m/%Y %H:%M:%S");

    var dataSourceCopy = [].concat(dataSource)
    dataSourceCopy.forEach(function(subData){
        var fixedDataCopy = [].concat(subData.data.fixedData); //array dupliation
        var animateDataCopy = [].concat(subData.data.animateData) // array duplication

        fixedDataCopy.forEach(function (d){
            if(xAxisIsTime===true){
                d.label = timeParse(d.label);
            }
        });
        subData.data.fixedData = fixedDataCopy

        animateDataCopy.forEach(function (d){
            if(xAxisIsTime===true){
                d.label = timeParse(d.label);
            }
        });
        subData.data.animateData = animateDataCopy
    });


    var combinedData = [] // combinedData used for increasing axis limit and limit calculation

    dataSourceCopy.forEach(function (d) {
        console.log("FIXED DATA"+d.data.fixedData);
        console.log("ANIMATED DATA"+d.data.animateData);
        combinedData = combinedData.concat(d.data.fixedData).concat(d.data.animateData);
    });

    var minX = combinedData[0].label;
    var maxX = combinedData[0].label;
    var maxValue = combinedData[0].value;

    for(var i=0;i<combinedData.length;i++){
        if(maxX<combinedData[i].label){
            maxX = combinedData[i].label;
        }
        if(maxValue<combinedData[i].value){
            maxValue = combinedData[i].value;
        }
    }
    

    var combinedData1 = [].concat(combinedData)
    combinedData1.push({"label":maxX,"value":(maxValue+(maxValue/10)),"type":"save"}); // adding extra value to extend the y axis beyond the highest data    

    var x
    // scale construction
    if(xAxisIsTime===true){
      x = d3.scaleTime()
          .rangeRound([0, width]);
    }else{
      x = d3.scaleLinear()
          .rangeRound([0, width]);
    }


    var y = d3.scaleLinear()
        .rangeRound([height, 0]);

    // gridlines in x axis function
    function make_x_gridlines() {
        return d3.axisBottom(x)
            .ticks(10);
    }

    // gridlines in y axis function
    function make_y_gridlines() {
        return d3.axisLeft(y)
            .ticks(5);
    }

 var line = d3.line().curve(d3.curveCardinal)
        .x(function (d) { return x(d.label); })
        .y(function (d) { return y(d.value); });
var lineArea = d3.area()
        .x(function(d){ return x(d.label); })
        .y0(y(0))
        .y1(function(d){ return y(d.value); })
        .curve(d3.curveCardinal);


    // domain construction
    x.domain([0,endTime]);
    //x.domain(d3.extent(combinedData1, function (d) { return d.label; }));
    y.domain(d3.extent(combinedData1, function (d) { return d.value; }));

    // drawing x axis
    if(xAxis){
      if(xAxisIsTime===true){
        g.append("g")
            .attr("transform", "translate(0," + height + ")")
            .call(d3.axisBottom(x).tickFormat(d3.timeFormat("%H:%M:%S")));
      }else{
        g.append("g")
            .attr("transform", "translate(0," + height + ")")
            .call(d3.axisBottom(x));
      }
    }



    // drawing y axis
    if(yAxis){
      g.append("g")
          .call(d3.axisLeft(y))
          .append("text")
          .attr("fill", "#000")
          .attr("transform", "rotate(-90)")
          .attr("y", 6)
          .attr("dy", "0.71em")
          .attr("text-anchor", "end");
    }


    // Draw gird lines
    // add the X gridlines
  
    //Drawing all the data
     var areaPath = g.append('path');
	var areaPath1 = g.append('path');
    //Drawing all the data
    var subData = dataSourceCopy[0]
    fixedDataCopy = subData.data.fixedData
    animateDataCopy = subData.data.animateData
	    // Draw new fill area
	    
        var fixedPath = g.append("path")
            .datum(fixedDataCopy)
            .attr("fill", "none")
            .attr("stroke", subData.lineColor)
            .attr("stroke-linejoin", "round")
            .attr("stroke-linecap", "round")
            .attr("stroke-width", 1.5)
            .attr("d", line);
        areaPath.datum(fixedDataCopy)
                .attr('class', 'area')
                .attr('d', lineArea);
        // drawing animateData line
        var animatePath = g.append("path")
            .datum(animateDataCopy)
            .attr("fill", "none")
            .attr("stroke", subData.lineColor)
            .attr("stroke-linejoin", "round")
            .attr("stroke-linecap", "round")
            .attr("stroke-width", 1.5)
            .attr("d", line);
        var animateArea=areaPath1.datum(animateDataCopy)
            .attr('class', 'area-animate')
            .transition()
            .delay(3000)
            .attr('d', lineArea);
        var animatePathTotalLength = animatePath.node().getTotalLength();

        // data line animation
        animateArea.attr("stroke-dasharray", animatePathTotalLength + " " + animatePathTotalLength)
            .attr("stroke-dashoffset", animatePathTotalLength)
            .transition()
            .duration(3000)
            .attr("stroke-dashoffset", 0);

            // drawing circles on data line
        /*  g.append("g")
            .selectAll("circle")
            .data(fixedDataCopy)
            .enter()
            .append("circle")
            .attr("cx", function (d) { return x(d.label); })
            .attr("cy", function (d) { return y(d.value); })
            .attr("fill", subData.circleColor)
            .attr("r", "3");

          g.append("g")
            .selectAll("circle")
            .data(animateDataCopy)
            .enter()
            .append("circle")
            .attr("cx", function (d) { return x(d.label); })
            .attr("cy", function (d) { return y(d.value); })
            .attr("fill", subData.circleColor)
            .transition()
            .delay(2000)
            .attr("r", "3");*/

          // adding icons to the points - fixed
          g.append("g")
              .selectAll("image")
              .data(fixedDataCopy)
              .enter()
              .append("image")
              .attr("x", function (d) { return x(d.label)-12; })
              .attr("y", function (d) {
                    return y(d.value)-20;
                })
              .attr("width", "40px")
              .attr("height","40px")
              // .transition()
              // .delay(2200)
              .attr("href", function(d) {
                for(i=0;i<subData.eventIconList.length;i++){
                  if(d.type===subData.eventIconList[i].name){
                    return subData.eventIconList[i].link
                  }
                }
              });
          // adding icons to the points - animate
          g.append("g")
              .selectAll("image")
              .data(animateDataCopy)
              .enter()
              .append("image")
              .attr("x", function (d) { return x(d.label)-12; })
              .attr("y", function (d) {
