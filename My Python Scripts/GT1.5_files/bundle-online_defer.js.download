function problemSideNavigation(winWidth, winHeight, displayDevice){
    var selectElement = document.querySelector('.problem-navigation');
    selectElement.style.height = winHeight - attemptTopSectionHeight() +'px';
    var getToggleBtnHeight = document.querySelector('.navbar-toggle-btn')?document.querySelector('.navbar-toggle-btn').offsetHeight:0;
    var getAttemptProblemLegend = document.querySelector('.problem-legend-details')?document.querySelector('.problem-legend-details').offsetHeight:0;
    var selectSmallProblemNavElement = document.querySelector('#smallProblemNavbar');
    selectSmallProblemNavElement.style.height = winHeight - (attemptTopSectionHeight() + getToggleBtnHeight)+'px';
    if(!document.getElementById('smallProblemNavbar').querySelector('.navbar-problem-outer')){
        eboxTopBottom({
            selector: '#smallProblemNavbar'
        });  
    }
    //Nav Menus child Selector 
    var problemMenuElement = document.querySelector('#problem-wrapper'), 
        sessionMenuElement = document.querySelector('#session-wrapper'),
        moduleMenuElement = document.querySelector('.module-wrapper'),
        sessionListElement = document.querySelector('.session-list-group'),
        sessionCompletion = document.querySelector('.session-completion'),
        finishBtnHeight = document.querySelector('.btn-section .finish-btn'),
        sectionNotLoaded = document.querySelectorAll('.not-loaded');

    //Nav Menus Selector
    var problemMenuParSelector = document.querySelector('.problem-detail'),
    sectionMenuParSelector = document.querySelector('.session-detail'),
    sessionMenuParSelector = document.querySelector('.session-category'),
    moduleMenuParSelector = document.querySelector('.module-detail');
       
    //Screen size more than 992 
    if(document.querySelector('#module-nav-link')){
        var dataUrlModule = document.querySelector('#module-nav-link').getAttribute('value');
    };
    if(document.querySelector('#session-nav-link')) {
        var dataUrlSession = document.querySelector('#session-nav-link').getAttribute('value');
    };
    if(displayDevice == 'md'){
        if(problemMenuElement && sessionListElement && sessionCompletion && finishBtnHeight){
            problemMenuElement.style.height = winHeight - (attemptTopSectionHeight() + getAttemptProblemLegend + 40)+'px';
            sessionListElement.style.height = winHeight - (attemptTopSectionHeight() + sessionCompletion.offsetHeight + finishBtnHeight.offsetHeight + getAttemptProblemLegend + 40) + 'px';
        }
        if(moduleMenuElement){
            moduleMenuElement.style.height = winHeight - (attemptTopSectionHeight() + getAttemptProblemLegend + 40)+'px';
        }
        if(sessionMenuElement){
            sessionMenuElement.style.height= winHeight - (attemptTopSectionHeight() + getAttemptProblemLegend + 40)+'px';
        }
        
        if(dataUrlModule && dataUrlSession ){
            document.querySelector('.session-category').style.display = 'block';
            if(moduleMenuParSelector.classList.contains('not-loaded') &&  !moduleMenuParSelector.querySelector('.section-menu-link')){
                moduleMenuParSelector.appendChild(createNavigationBtn('#module-content', 'Module','', dataUrlModule,'100%'));
            }
            if(sessionMenuParSelector.classList.contains('not-loaded') && !sessionMenuParSelector.querySelector('.section-menu-link')){
                sessionMenuParSelector.appendChild(createNavigationBtn('#session-content', 'Sessions', '',dataUrlSession,'100%'));
            }
            if(sectionNotLoaded.length > 0){
                for(var i = 0;i < sectionNotLoaded.length;i++){
                    sectionNotLoaded[i].style.height= winHeight - (attemptTopSectionHeight() + getAttemptProblemLegend + 1)+'px';     
                }   
            }  
            document.querySelectorAll('.section-menu-link').forEach(function(element){
                element.onclick = function(event){event.preventDefault();loadSectionMenus(winWidth, winHeight, this, displayDevice)};
            });
            document.querySelectorAll('.module-link-action').forEach(function(element){
                element.onclick = function(event){event.preventDefault();loadSectionMenus(winWidth, winHeight, this, displayDevice)};
            });
        } 
        document.querySelectorAll('.section-link-action').forEach(function(element){
            element.onclick = function(event){event.preventDefault();loadProblemMenus(this, displayDevice)};
        });
    }

    if(displayDevice == 'sm'){
        selectElement.classList.add('sm');
        if(problemMenuElement && sessionListElement && sessionCompletion && finishBtnHeight){
            problemMenuElement.style.height = winHeight - (attemptTopSectionHeight() + getAttemptProblemLegend + 40)+'px';
            sessionListElement.style.height = winHeight - (attemptTopSectionHeight() + sessionCompletion.offsetHeight + finishBtnHeight.offsetHeight + getAttemptProblemLegend + 78) + 'px';
