function applyOptionalCustomTags(divname,fieldname){
    
	var inputString = new String(document.getElementById(divname).innerHTML);
	if(BrowserDetect.browser=="Chrome") {
		//inputString = inputString.replace(/<div>/g,'<br>').replace(/<\/div>/g,''); //-Chrome
                inputString = inputString.replace(/<div>/g,'<br>').replace(/<\/div>/g,'').replace(/&nbsp;/g,' '); //-Chrome
		inputString = inputString.replace(/<\/b>(<br>)+<b>/g,'<br>');
	}
	else {
		var len = (inputString.length-"<br>".length);
		if(inputString.substr(len) == "<br>")
			inputString = inputString.substr(0,inputString.length - "<br>".length); //- Mozilla
	}
	inputString = inputString.replace(/<b>/g,'<optional>').replace(/<\/b>/g,'</optional>').replace(/<br>/g,'\n');
    document.getElementById(fieldname).value=inputString;
}

