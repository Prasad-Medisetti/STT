(function(){
    'use strict';
    var options, TopBottomProperty, el;
    var topBottomInitial = function(options){
        this.defaults = {
            selector : '#smallProblemNavbar',
            upArrow : '<span class="eboxicon eboxicon-caret-up"></span>', 
            downArrow : '<span class="eboxicon eboxicon-caret-down"></span>'
        };
        options = extend(this.defaults, options);
        if(typeof options.selector === 'string'){
            el = document.querySelector(options.selector);
            this.createNavToElement(el, options);
        } else {
            return false;
        }
    }
    /* Extend the Property */
    TopBottomProperty = topBottomInitial.prototype;
    TopBottomProperty.createNavToElement = function(el, options){          
        var parentHTML = document.createElement('div');
        parentHTML.className = 'navbar-problem-group';
        for(var i=0; i < el.childNodes.length; i++){
            parentHTML.appendChild(el.childNodes[i]);
            el.childNodes[0].parentNode.insertBefore(parentHTML, el.childNodes[0]);
        }
        var outerDiv = document.createElement('div');
        outerDiv.className = 'navbar-problem-outer';
        outerDiv.appendChild(el.childNodes[0]);
        el.insertBefore(outerDiv, el.childNodes[0]);
        var listHeight = document.querySelector('.navbar-problem-outer').offsetHeight, elHeight =  el.offsetHeight;

        if(listHeight > elHeight){
            document.querySelector('.navbar-problem-outer').style.height = (elHeight - 60) + 'px';
            this.appendArrow(el, options);
            var problemAction = el.querySelector('.problem-list.active');
            if(problemAction){
                if(problemAction.offsetTop > (elHeight - 60)){TopBottomProperty.ScrollActiveProblem(el, (elHeight - 60))}
            }
            document.querySelectorAll(".side-nav-arrow.up-arrow")[0].addEventListener("click",function(){TopBottomProperty.moveUp(el)});
            document.querySelectorAll(".side-nav-arrow.down-arrow")[0].addEventListener("click",function(){TopBottomProperty.moveDown(el)});
        }
    };
    TopBottomProperty.appendArrow = function(el, options){
        
        el.insertBefore(createUpArrow(options.upArrow), el.firstChild);
        el.insertBefore(createDownArrow(options.downArrow), el.lastChild);
    };
    TopBottomProperty.ScrollActiveProblem = function(el, navOuterHeight){
        var activePostion = el.querySelector('.problem-list.active').offsetTop,
            M_el = el.getElementsByClassName('navbar-problem-group')[0], 
            getHeight = getItemHeight(el);
            M_el.style.transform = "translateY("+((navOuterHeight - activePostion) - getHeight) +"px)";
    }
    TopBottomProperty.moveUp = function(el){
        var M_el = el.getElementsByClassName('navbar-problem-group')[0], getPosition = getStyles(M_el), getHeight = getItemHeight(el);
        if(getPosition < 0 ){
            if((getPosition + getHeight) < 0){
                M_el.style.transform = "translateY("+(getPosition + getHeight)+"px)";
            } else {
                M_el.style.transform = "translateY(0px)";
            }
        }
    };
    TopBottomProperty.moveDown = function(el){
        var M_el = el.getElementsByClassName('navbar-problem-group')[0], getPosition = getStyles(M_el), getHeight = getItemHeight(el);
        if((document.querySelector('.navbar-problem-outer').offsetHeight - document.querySelector('.navbar-problem-group').offsetHeight) < getPosition){    
            if((document.querySelector('.navbar-problem-outer').offsetHeight - document.querySelector('.navbar-problem-group').offsetHeight + getHeight) < getPosition){
                M_el.style.transform = "translateY("+(getPosition - getHeight)+"px)";
            } else {
                M_el.style.transform = "translateY("+(document.querySelector('.navbar-problem-outer').offsetHeight - document.querySelector('.navbar-problem-group').offsetHeight)+"px)";
            }
        }
    };
    /* Helper Function */
    function extend(obj, src) {
        for (var key in src) {
            if (src.hasOwnProperty(key)) obj[key] = src[key];
        }
        return obj;
    }
    function createUpArrow(arrowElement){
        var arrowHTML = document.createElement("div");
        arrowHTML.className = 'side-nav-arrow up-arrow';
        arrowHTML.innerHTML = arrowElement;
        return arrowHTML;
    }
    function createDownArrow(arrowElement){
        var arrowHTML = document.createElement("div");
        arrowHTML.className = 'side-nav-arrow down-arrow';
        arrowHTML.innerHTML = arrowElement;
        return arrowHTML;
    }
    function getStyles(obj){
        if(!window.getComputedStyle) return;
        var style = getComputedStyle(obj),
            transform = style.transform || style.webkitTransform || style.mozTransform;
        var mat = transform.match(/^matrix3d\((.+)\)$/);
        if(mat) return parseFloat(mat[1].split(', ')[13]);
        mat = transform.match(/^matrix\((.+)\)$/);
        return mat ? parseFloat(mat[1].split(', ')[5]) : 0;
    }

    function getItemHeight(el){
        return el.getElementsByClassName('navbar-problem-group')[0].getElementsByTagName('div')[0].offsetHeight;
    }
    
    window.eboxTopBottom = function (option) {        
        if(!option || !window.addEventListener){return false;}
        return new topBottomInitial(option);
    };

})();


