function verifyEmailId(url) {
    $("#validation-error-email").html("");
    var email = $("#email").val().trim();
    var flag = true;
    var re = /^(([^<>()\[\]\\.,;:\s@"]+(\.[^<>()\[\]\\.,;:\s@"]+)*)|(".+"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;
    if(re.test(email)){ 
        $.ajax({
            url: url,
            type: 'POST',
            async: false,
            data: {
                "email":email
            },
            success: function(data) {
                if(data.email || data.isVerifiedMail){
                    $(".error-mail").show();
                    var msg = "Email id is already exist";
                    if(data.isVerifiedMail){
                        msg = "Email id is already verified";
                    }
                    $(".error-mail").html(msg);
                    $('#tick').hide();
                    $('#mail-verified').hide();
                    $(".verifyMail").attr("disabled", true);
                    flag = false;
                }
                else if(!data.email){
                    $(".error-mail").hide();
                    $('#verifyMail').show();
                    $('#tick').show();
                    $(".verifyMail").attr("disabled", false);
                }
                else if(!data.isVerifiedMail){
                    $(".error-mail").html("");
                }

            }
        });
    }
    else{
        $(".error-mail").html("Enter a valid email id");
        $('#mail-verified').hide();
        flag = false;
    }
    return flag;
}

