                            } else {
                                $("#userEventButton").val("Accept");
                                $("#userEventButton").attr("class","btn btn-success");
                            }
                        } else {
                            $("#userEventButton").remove();
                        }
                        $('#fullCalModal').modal('show');                        
                        return false;
                    }
                    cal.viewDisplay = function (view) {
                        
                    }
          
                    $('#courseCalendar').fullCalendar(cal);
                
            },
            error: function (xhr, ajaxOptions, thrownError) {
                console.log(xhr.status);
                console.log(thrownError);
            }
     });
