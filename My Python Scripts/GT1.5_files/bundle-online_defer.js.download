function validateMileStone() {    
    var name = $("#name").val();
    var duration = $("#duration").val();
    var checkedValues = $('input:checkbox:checked').map(function() {
        return this.value;
    }).get();
    var status = false;
    if(name =="") {
        alert("Please fill the Name");
        status = false;
    }
    else if(duration == -1){
        alert("Please Select the Days");
        status = false;
    }
    else if(checkedValues.length == 0) {
