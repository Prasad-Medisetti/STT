function loadMentorItem(e,element){
    e.preventDefault();
    var mentorId = Number($(element).attr('mentor-id'));
    var inputCheckBox = $('.tui-full-calendar-checkbox-square.check-mentor')[0];
    var scheduleData = cal._controller.schedules.items;
    if($(element).hasClass("active")){
        selectedMentorIdList = selectedMentorIdList.filter(function(ele){
            return ele !== mentorId;
        });
        for (var key in scheduleData) {
            if(scheduleData[key].assigneId === ""+mentorId){
                $("div[data-schedule-id='"+scheduleData[key].id+"']").parent().hide();
            }
        }
        $(element).removeClass("active");
    }
    else{
        selectedMentorIdList.push(mentorId);
        $(element).addClass("active");
        for (var key in scheduleData) {
            if(scheduleData[key].assigneId === ""+mentorId){
                $("div[data-schedule-id='"+scheduleData[key].id+"']").parent().show();
            }
        }

    }
    inputCheckBox.checked = ($('.lnb-mentors-item').length === selectedMentorIdList.length);
    if(inputCheckBox.checked)
        $(inputCheckBox).parent().addClass('active');
    else
        $(inputCheckBox).parent().removeClass('active');
    if(selectedMentorIdList.length === 0){
        for (var key in scheduleData) {
            $("div[data-schedule-id='"+scheduleData[key].id+"']").parent().hide();
        }
    }
    if(selectedMentorIdList.length === $('.lnb-mentors-item').length){
        for (var key in scheduleData) {
            $("div[data-schedule-id='"+scheduleData[key].id+"']").parent().show();
        }        
    }    
}

