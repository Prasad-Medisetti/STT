function openCourseCalendar() {    
    $.ajax({
            url: contextPath+"/course/courseSchedule",
            type: "POST",
            before : showWait(),
            success : function(data){
                hideWait();
                $("#myModalBody").html("<div id='courseCalendar'> </div>");
                
                var cal=JSON.parse(data);
                    for (var i in cal.events){
                        if(cal.events[i].start)
                            cal.events[i].start = eval(cal.events[i].start)
                        if(cal.events[i].end)
                            cal.events[i].end = eval(cal.events[i].end)
                    }
                    cal.eventClick = function(event) {
                        // opens events in a popup window                        
                        $("#fullCalEventName").html(event.title);
                        $("#fullCalStartDate").html($.fullCalendar.formatDate(event.start, 'dd/MM/yyyy HH:mm:ss'));                        
                        $("#fullCalEndDate").html($.fullCalendar.formatDate(event.end, 'dd/MM/yyyy HH:mm:ss'));
                        $("#fullCalEventDetails").html(event.description);
