function markMessageAsValid(validMessage,id) {
    $('#edit-message').hide();
    $("#valid-message").show();
    $('#validation-error').hide();
    var message = ""
    if($('#msg'+id).prop("checked") == true){
        message =   $("#messages").text() + validMessage
    } else if($('#msg'+id).prop("checked") == false) {
        message = $("#messages").text().replace(validMessage,"");
    }
    var selectedMsg = "<div class='edit-icon' onclick=\"(editSelectedMsg('" + message.replace(/\n/g, '\\n').replace(/'/g, "\\'").replace( /"/g, '&quot;' ) + " '))\"><span class='fa fa-edit' style='float: right;cursor:pointer'></span></div>";
    selectedMsg+= "<pre id='selected-messages'></pre>"
    if(message.indexOf("\n",1)!=-1) {
        selectedMsg += "<span class='fa fa-window-maximize code-align' onclick=\"jQuery(this).toggleClass('fa-window-maximize');jQuery(this).toggleClass('fa-window-minimize');jQuery(this).prev().toggleClass('display-code');\"></span>";
    }
    $('#messages').html(selectedMsg);
    $('#selected-messages').append(message.escapeHTML());
}

function editSelectedMsg(selectedMsg) {
    $("#valid-message").hide();
    $('#edit-message').show();
    $('#selected-msg').val(selectedMsg);
}

function messageCheck() {
    if(($('#selected-msg').val().trim())) {
        hideConversationModal();
        return true;
    } else {
        $('#validation-error').show();
        $('#validation-error').html('Message cannot be empty')
        return false;
    }
    
}
