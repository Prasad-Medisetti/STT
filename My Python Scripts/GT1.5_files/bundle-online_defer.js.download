        isDoubtThreadLoading = true;
        $('#small-spinner').jmspinner();
        doubtThreadUsernameSearchRequest = jQuery.ajax({
            method:'post',
            async:false,
            url:contextPathURL+'/doubtThread/obtainDoubtThreadByUsername',
            data:{'username' : userName, offset : loadedDoubtThreadOffset},
