function  strToDate(datetime) {
    var splitdate = datetime.split(" ");
    var date = splitdate[0].split("-")[0];
    var month = splitdate[0].split("-")[1];
    var year = splitdate[0].split("-")[2];
    var hours = splitdate[1].split(":")[0];
    var minutes = splitdate[1].split(":")[1];
    if (splitdate[2] === "PM" && hours < 12) {
        hours = parseInt(hours) + 12;
    }
    return  new Date(year, month - 1, date, hours, minutes);
}

