        autoRequestMedia: (videoEnabled == 1 || audioEnabled == 1)?true:false,
        autoRemoveVideos:true,
//        detectSpeakingEvents: (videoEnabled == 1 || audioEnabled == 1)?true:false,
        // dont negotiate media
