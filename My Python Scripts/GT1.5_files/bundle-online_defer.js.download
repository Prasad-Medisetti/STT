function countTimerTutoring(sessDate,id){
    var t1 = sessDate.split(" ");
    var dat = t1[0].split("-").join("/");
    var t2 = t1[1].split(".");
    var final = dat+" "+t2[0];
    var todayTime = new Date(final);
    var countDownDate = todayTime.getTime();
    var x = setInterval(function() {
    var now = new Date().getTime();
    var distance = countDownDate - now;
    var days = Math.floor(parseFloat(distance / (1000*60*60*24)));
    var hours = Math.floor((distance % (1000*60*60*24)) / (1000*60*60));
    var minutes = Math.floor(parseFloat((distance % (1000*60*60)) / (1000*60)));
    var seconds = Math.floor(parseFloat((distance % (1000*60)) / 1000));
        $("#time-rem"+id).html((days >= 1) ?(days+"d "):"");
        $("#time-rem"+id).append((hours >= 1) ?(hours+"h "):"");
        $("#time-rem"+id).append(minutes+"m ");
        $("#time-rem"+id).append(seconds+"s");
        $("#time-rem"+id).show();
        if (distance < 0) {
            clearInterval(x);
            $("#timer-nav").hide();
            $(".join-now"+id).show();
        }
    }, 1000);
}

