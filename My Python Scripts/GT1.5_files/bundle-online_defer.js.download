function expandCircuitEditor(){
    $(".ebox-popover").hide();
    var element = document.querySelector('.expand-editor-content');
    element.classList.toggle("full-screen");
    var hideElement = document.querySelector('.problem-question')
    hideElement.classList.toggle('hide');
    var resizer = document.querySelector('#resizer')
    resizer.removeAttribute('style');
    resizer.classList.toggle('hide');    
    var attemptWrapper = document.querySelector('.attempt-wrapper')
    attemptWrapper.classList.toggle('hide');
    var getClass = this.querySelector('span').getAttribute('class').split(' ')[1]
    var navId = document.querySelector('#problem-navId');
    if(getClass == 'expand-icon'){
        var selectIcon = this.querySelector('.ebox-icon');
        selectIcon.classList.remove('expand-icon');
        selectIcon.classList.add('expand-close-icon');
        navId.classList.add('hide');    
        document.querySelector('.program-wrapper #right').style.height = (window.innerHeight-headSectionHeight()) +'px';
        document.querySelector('.program-wrapper #right').style.width = window.innerWidth +'px';
        document.querySelector('#editor-scroll').style.textAlign = "center";
    }
    if(getClass == 'expand-close-icon'){
        var selectIcon = this.querySelector('.ebox-icon');
        selectIcon.classList.remove('expand-close-icon');
        selectIcon.classList.add('expand-icon');
        navId.classList.remove('hide');    
        setSizeCircuitEditor(window.innerWidth, window.innerHeight-headSectionHeight(), 0);
        document.querySelector('#editor-scroll').style.textAlign = "left";
    }
}
