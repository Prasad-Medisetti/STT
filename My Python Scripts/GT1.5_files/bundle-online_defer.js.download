function loadSectionMenus(winWidth, winHeight,el, displayDevice){
    if((el.getAttribute('href')== 'session-category'|| el.getAttribute('href')== 'module-detail') && 
        (document.querySelector('.'+el.getAttribute('href')).getAttribute('data-loaded') == 'false') || 
        (el.getAttribute('data-parent-class')== 'module-detail') || (el.getAttribute('href')== '#module-content') || 
        (el.getAttribute('href') == '#session-content')){
        $.ajax({         
            url: el.getAttribute('data-url'),        
            before: showWait(),
            success: function(data) {
                if(displayDevice == 'md')
                {
                    el.getAttribute('href') == 'session-category' ? $('.'+el.getAttribute('href')+' #session-content').html(data):$(el.getAttribute('href')).html(data);
                    el.parentElement.classList.remove('not-loaded');
                    if(el.parentElement.previousElementSibling) el.parentElement.previousElementSibling.style.display = 'block';
                    if(!el.closest('.'+el.getAttribute('data-parent-class'))){
                        el.parentElement.querySelector(el.getAttribute('href')).style.display = "block";
                        el.remove();
                    }
                    if(el.closest('.'+el.getAttribute('data-parent-class'))){
                        document.querySelectorAll('.module-detail .problem-list').forEach(function(element){
                            element.classList.remove('active');
                        });
                        el.closest('.problem-list').classList.add('active');
                    }
                    problemSideNavigation(winWidth, winHeight, displayDevice); 
                } else {
                    el.getAttribute('href') == 'session-category' ? $('.'+el.getAttribute('href')+' #session-content').html(data): $('.'+el.getAttribute('href')+' #module-content').html(data);        
                    document.querySelector('.'+el.getAttribute('href')).setAttribute('data-loaded','true');
                    if(el.closest('.'+el.getAttribute('data-parent-class'))){
                        document.querySelectorAll('.module-detail .problem-list').forEach(function(element){
                            element.classList.remove('active');
                        });
                        el.closest('.problem-list').classList.add('active');
                        el.closest('.'+el.getAttribute('data-parent-class')).style.display = 'none';
                    } else {
                        el.parentNode.parentNode.style.display = 'none';
                    }
                    document.querySelector('.'+el.getAttribute('href')).classList.remove('not-loaded');
                    document.querySelector('.'+el.getAttribute('href')).style.display = "block";
                    document.querySelector('#module-content').style.display = 'block';
                    document.querySelector('#session-content').style.display = 'block';
                    problemSideNavigation(winWidth, winHeight, displayDevice); 
                }
                hideWait();
            },
            error: function(data) {
                hideWait();
            }
        }); 
    } else{
        if(displayDevice == 'md'){
            el.parentElement.classList.remove('not-loaded');
            if(el.parentElement.previousElementSibling) el.parentElement.previousElementSibling.style.display = 'block';
            el.parentElement.querySelector(el.getAttribute('href')).style.display = "block";
            el.remove();
            problemSideNavigation(winWidth, winHeight, displayDevice); 
        } else {
            el.parentNode.parentNode.style.display = 'none';
            document.querySelector('.'+el.getAttribute('href')).classList.remove('not-loaded');
            document.querySelector('.'+el.getAttribute('href')).style.display = "block";
            document.querySelector('#module-content').style.display = 'block';
            document.querySelector('#session-content').style.display = 'block';
            problemSideNavigation(winWidth, winHeight, displayDevice);           
        }   
    }
}
