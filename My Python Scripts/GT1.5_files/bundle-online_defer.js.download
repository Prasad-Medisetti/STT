function amphiSortable(mySelector,AfterDrop,swap,exclude){
      ////console.log(amphiSortablecontainerGroups)
      var onDrop = function  ($item, targetContainer, _super) {
            $item.removeClass('cantDrop')
            $item.removeClass('canDrop')
            var clonedItem = $('<li/>').css({
                  height: 0
            })
            $item.before(clonedItem)
            clonedItem.animate({
                  'height': $item.height()
            })
            $item.animate(clonedItem.position(), function  () {
                  clonedItem.detach()
                  _super($item)
                  if(swap){
                    var dropPosition = $item.index();
                    if(dragAndDropAdjustment.startingPosition > dropPosition){
                        $($item.parent().children()[dragAndDropAdjustment.startingPosition]).after($($item.parent().children()[dropPosition+1]).clone());
                        $($item.parent().children()[dropPosition+1]).remove();
                    }
                    else if(dragAndDropAdjustment.startingPosition < dropPosition){
                        $($item.parent().children()[dragAndDropAdjustment.startingPosition]).before($($item.parent().children()[dropPosition-1]).clone());
                        $($item.parent().children()[dropPosition]).remove();
                    }
                  }
                  if(AfterDrop){
                        AfterDrop($item);
                  }
            })
      };
    
      var onDragStart = function ($item, container, _super) {
            var offset = $item.offset(),
            pointer = container.rootGroup.pointer
            dragAndDropAdjustment = {
                  left: pointer.left - offset.left,
                  top: pointer.top - offset.top,
                  startingPosition:$item.index()
            }      
            //console.log(container)
            _super($item, container);
      };
      var onDrag = function ($item, position) {
            if($(document).find("li.placeholder").length>0){
                  $item.removeClass('cantDrop')
                  $item.addClass('canDrop')
            }
            else{
                  $item.removeClass('canDrop')
                  $item.addClass('cantDrop')
            }
            $item.css({
                  left: position.left - (sortableMobile?0:dragAndDropAdjustment.left),
                  top: position.top - (sortableMobile?0:dragAndDropAdjustment.top)
            });
      };
      var obj = {
            group:mySelector,
            pullPlaceholder:false,
            onDrop:onDrop,
            onDragStart:onDragStart,
            onDrag:onDrag
      };
      if(exclude){
        obj.exclude = exclude;
       }
      $(mySelector).sortable(obj);
}

function getTreeChildren(item,items){
      var span=$("<span />").html(item.name);
      if(item.label != undefined){
        var label = $(" <span />").html(" - "+item.label.text); 
        //var lclass = 'treelabel-'+item.label.class;
        //label.addClass(lclass);
        label.attr("title",item.label.title);
        span.append(label);
      }
      for(var property in item){
          if(property!='children'){
              span.attr(property,item[property]);
          }
      }
      span.addClass("tree-item");
      if(span.attr("title") == undefined && span.attr("name") != undefined){
          span.attr("title",span.attr("name"))
      }
      var ul=$("<ul />");
      if(item.children!=undefined && item.children.length>0){
            span.addClass("folder");
            for(var i in item.children){
                  var children=item.children[i];
                  if(children._reference!=undefined){
                        for(var j in items){
                              if(items[j].id==children._reference){
                                    children=items[j];
                                    break;
                              }
                        }
                  }
                  ul.append(getTreeChildren(children,items));
            }
      }
      else{
            span.addClass("file");
      }
      var li=$("<li />").append(span);
      if(item.children!=undefined && item.children.length>0)
            li.append(ul);
      return li;
}

function getTree(items,query){
      var ul=$("<ul />");
      for(var i in items ){
          if(query!=null){
            for(var j in query){
                  if(items[i][j]==query[j]){
                        ul.append(getTreeChildren(items[i],items));
                        break;
                  }
            }
        }
        else{
            ul.append(getTreeChildren(items[i],items));
        }
      }
      return ul;
}

