    eboxApp.prototype.editorialScreen = function(options){
        if(options.selector){ var selector = document.querySelector(options.selector);}
        if(options.setHeight  === true){
            var getBottomHeight = selector.querySelector('.editorial-bottom') ? parseInt(selector.querySelector('.editorial-bottom').offsetHeight, 10) : 0;
            var editorialVideoPadding = {'top':parseInt(getComputedStyle(selector).paddingTop, 10), 'bottom':parseInt(getComputedStyle(selector).paddingBottom, 10)};
            if(selector.querySelector('.editorial-top')){selector.querySelector('.editorial-top').style.height = window.innerHeight - (getBottomHeight + editorialVideoPadding.top + editorialVideoPadding.bottom + this.settings.headerHeight + 40)+'px'};
        }
        if(options.selector && selector.querySelector(options.animationSelector) && selector.querySelector('.editorial-text-animation') && document.querySelector('#showHelp').value == "true"){
            selector.querySelector(options.animationSelector).style.width = '100%';
            selector.querySelector('.editorial-text-animation').style.opacity = '1';
            setTimeout(function(){
                selector.querySelector(options.animationSelector).style.right = '0';
                selector.querySelector(options.animationSelector).style.left = 'inherit';
                selector.querySelector(options.animationSelector).style.width = '0';
            }, options.animationTime)
        }
        else if(document.querySelector(options.animationAlternateSelector) && document.querySelector('.editorial-text-animation') && document.querySelector('#showHelp').value == "true"){            
            document.querySelector(options.animationAlternateSelector).style.width = '100%';  
            document.querySelector(options.animationAlternateSelector).querySelector('.editorial-text-animation').style.opacity = '1';
        }
        document.querySelector('#showHelp').value = "true"
    }
    
    eboxApp.prototype.timeCounter = function(timerOptions){
        if (timerOptions) {
            var sumTime = 0;
            timerOptions.distance = timerOptions.problemTime * 1000 - timerOptions.currentTime * 1000;
            timeCounterElement(timerOptions.distance, timerOptions.selector);
            timerOptions.intervalObj = setInterval(function() {
                timerOptions.distance -= 1000;
                sumTime += 1000;
                timeCounterElement(timerOptions.distance, timerOptions.selector);
                if(sumTime > timerReloadTime){
                    updateTimer(timerOptions);
                    sumTime = 0;
                }
                if (timerOptions.distance < 1) {
                    var endCheck = lastCheckRemainingTime;
                    if (endCheck != undefined && typeof (endCheck) == "function") {
                        endCheck(timerOptions);
                    }
                    else{
                        clearInterval(timerOptions.intervalObj);
                        try {
                            var endCallBack = timeEnd;
                            if (endCallBack != undefined && typeof (endCallBack) == "function") {
                                endCallBack();
                            }
                        }
                        catch (e) {
                            console.log("timeEnd callback err", e)
                        }
                    }
                }
            }, 1000)
        }
        return timerOptions;
    }
