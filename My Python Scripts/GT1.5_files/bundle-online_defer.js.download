function dragResizer(){
    var leftWidth, RightWidth, initialPostionLeft, positionleft;
    $('#resizer').draggable({
        axis: "x",
        containment: 'parent',
        start:function(event, ui){
            initialPostionLeft = ui.position.left;
            leftWidth = $('#left').width();
            RightWidth = $('#right').width();
        },
        drag:function(event, ui){
            positionleft = ui.position.left;
            var leftSetWidth = leftWidth + positionleft - initialPostionLeft;
            var rightSetWidth = RightWidth - positionleft + initialPostionLeft;
            if(leftSetWidth < 250 || rightSetWidth < 550 ){
                return false;
            } else {
                $("#left").width(leftWidth + positionleft - initialPostionLeft);
                $("#right").width(RightWidth - positionleft + initialPostionLeft);
            }
            editorBtnActionStyle(RightWidth - positionleft + initialPostionLeft);
            var th = headSectionHeight();
            var bh = bottomPostHeight();
            var footerHeight = document.querySelector('#footer')?document.querySelector('#footer').offsetHeight:0;
            if(document.querySelector('.program-wrapper #editor-scroll')){setScrollHeight(window.innerHeight, th+bh+footerHeight)};
        }
    });
}
function verticalDragResizer(){
    var topWidth, bottomWidth, initialPostionTop, positionTop;
    $('#v-resizer').draggable({
        axis: "y",
        containment: 'parent',
        start:function(event, ui){
            initialPostionTop = ui.position.top;
            topWidth = $('#top').height();
            bottomWidth = $('#bottom').height();
        },
        drag:function(event, ui){
            positionTop = ui.position.top; 
            $("#top").height(topWidth + positionTop - initialPostionTop);
            $("#bottom").height(bottomWidth - positionTop + initialPostionTop);
            if(document.querySelector('.program-wrapper #editor-scroll')){setBottomScrollHeight(bottomWidth - positionTop + initialPostionTop, 0)};
        }
    });
}
function timeCounterElement(distance, selector){
    var selectElement = document.querySelector(selector);
    selectElement.classList.add('time-counter');
    selectElement.innerHTML =  '<div class="hours"><span class="counter-text">' + timeHoursSpliter(distance) + '</span></div><div class="mins"><span class="counter-text">' +timeMinutesSpliter(distance)+'</span></div><div class="seconds"><span class="counter-text">'+ timeSecondsSpliter(distance) +'</span></div>';
}     
function timeHoursSpliter(distance){
        var h = Math.floor((distance % (1000 * 60 * 60 * 24)) / (1000 * 60 * 60));
        return h.toString().length == 2 ? h : '0'+h; 
}
function timeMinutesSpliter(distance){
        var m = Math.floor((distance % (1000 * 60 * 60)) / (1000 * 60));
        return m.toString().length == 2 ? m : '0'+m; 
}
function timeSecondsSpliter(distance){
        var s = Math.floor((distance % (1000 * 60)) / 1000);
        return s.toString().length == 2 ? s : '0'+s; 
}
