function showNotificationDialog(teamVis,isTimeEnded,modVis,isStarted) {
    var teamVisibility = teamVis;
    var moduleVisibility = modVis;
   
    if(moduleVisibility == true){
        if(teamVisibility == true && isTimeEnded == false && isStarted == true){
            document.getElementById("infoMsgContent").innerHTML = "Module and Session are visible, but the scheduled time is not yet started for this team. So the team participants will not be able to attempt this session.";
        }
        else if(teamVisibility==true && isTimeEnded==true){
                document.getElementById("infoMsgContent").innerHTML = "Module and Session are visible, but the scheduled time got ended. So the team participants will not be able to attempt this session.";
        }
        else if(teamVisibility==false){
            document.getElementById("infoMsgContent").innerHTML = "Module is visible, but the session is invisible. So the team participants will not be able to attempt this session.";
        }
    }  
    else if(moduleVisibility == false){
        document.getElementById("infoMsgContent").innerHTML = "Module is invisible. So the team participants will not be able to attempt this session.";
    }            
    jQuery('#infoMsgDialog').modal('show');
    $('#infoMsg').modal('show');          
}

