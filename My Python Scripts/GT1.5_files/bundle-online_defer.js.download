                $('#notreadicon-'+doubtThreadId).hide();
                $('#doubt-not-replied-status-'+doubtThreadId).hide();
                if(respondData.doubtStatusName !== undefined && respondData.doubtStatusName.length > 0){
                    var color = respondData.doubtStatusName === "Re-opened" ? "#c60000" : respondData.doubtStatusName === "Opened" ? "#ffa511":"#888888"; 
