function cropImage(input){
    $('#profilePicture').cropper("destroy");
    // call a method via jquery
    if (input.files && input.files[0]) {
        var reader = new FileReader();
        reader.onload = function (e) {
            $('#profilePicture').attr('src', e.target.result).fadeIn('slow');
            $('#profilePicture').cropper({
                aspectRatio:1/1,
                viewMode: 2,
                dragMode:'move',
                background:false,
                cropBoxResizable:false,
                toggleDragModeOnDblclick:false, 
                maxCropBoxWidth: 200, 
                maxCropBoxHeight: 200,
                minCropBoxWidth: 200, 
                minCropBoxHeight: 200,
                minCanvasWidth:0,
                minCanvasHeight:0
                    
            });
            
            // Get the Cropper.js instance after initialized
            $('#profilePicture').data('cropper');
            $('#display-profile').hide();
            $('#preview-image').hide();
            $('#crop-image').show();
            $('#saveButton').show();
        }
        reader.readAsDataURL(input.files[0]);
    }
}
