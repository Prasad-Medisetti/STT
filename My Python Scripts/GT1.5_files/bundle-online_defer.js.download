function alertActivitySchedule(content){
    var targetUrl = "", btnMessage = "", joinUrl = content.data.joinUrl;
    content.data.minutes += 1; 
    if(content.data.minutes  > 0){
        if(content.data.activityType === 'Tutoring Session'){
            targetUrl = contextPath+'/tutoring/viewAllBatches?batchId='+content.tutoringBatchId;
            btnMessage = "View Details";
        }
    }else{
        targetUrl = content.data.joinUrl;
        btnMessage = "Join Now";
    }
    var data = [
        '<div class="modal-data-activity-schedule tutoring-session-notification '+(content.data.activityType === 'Master Class' ? 'master-class':'')+'">',
            '<button class="close-btn" onclick="removeDataActivity(this)" data-id="'+content.data.activityId+'"><span class="eboxicon eboxicon-close"></span></button>',
            '<p>'+content.message+'</p>',
            '<div class="tutoring-timer tutoring-timer-'+content.data.activityId+'" style="display:none">',
                '<span class="ebox-icon ebox-tut-loading"></span>',
                '<span id="tutoring-timer-'+content.data.activityId+'" data-id="'+content.data.activityId+'" class="time" style="display:inline-flex;">30 : 00</span>',
                '<span style="margin-left:5px;"> mins left</span>',
            '</div>',
            '<div class="btn-section" style="display:'+(btnMessage === "" ? 'none':'block')+';">',
                '<a target="__blank" joinurl="'+joinUrl+'" href="'+targetUrl+'" id="tutoring-remind-me-'+content.data.activityId+'" class="btn btn-primary">'+btnMessage+'</a>',
            '</div>',
            '<ul class="inline-group" style="display: none;">',
                '<li class="inline-list"><button data-value="5 Mins">5 Mins</button></li>',
                '<li class="inline-list"><button data-value="10 Mins">10 Mins</button></li>',
            '</ul>',
        '</div>'
    ].join('\n');
    $("body").append(data);
    setTimeout(function(){
        $('.modal-data-activity-schedule').addClass('popup-shown');
        if (content.data.minutes < 1440 && content.data.minutes > 0){
            eboxApp.timeCounter({
                selector:'#tutoring-timer-'+content.data.activityId,
                problemTime: (new Date().getTime() / 1000) + content.data.minutes*60,
                currentTime: new Date().getTime() / 1000
            });
            $(".tutoring-timer-"+content.data.activityId).show();
        }
        else{
            $(".tutoring-timer-"+content.data.activityId).hide();
        }
    },1000);
    
    var closeModalDataActivity = setInterval(function(){
        if($('.modal-data-activity-schedule:hover').length === 0){
            //$('.modal-data-activity-schedule').removeClass('popup-shown');
            //clearInterval(closeModalDataActivity);
        }
    },10000);
}

function loadActivitySchedule(url, timeOut){
    setTimeout(function(){
        $.ajax({
            url : url,
            method : "POST",
