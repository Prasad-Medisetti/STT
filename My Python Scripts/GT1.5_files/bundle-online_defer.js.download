function searchBoxWithCategory() {    
    $("select.amphiSearchSelectBox").each(function(){
        var $this = $(this);        
        if($this.attr("amphi-search-data") != undefined && $this.attr("data-loaded") != "true" && $this.attr("amphi-search-data").length > 0){            
            var jsonResponse = JSON.parse($this.attr("amphi-search-data"));
            $(this).attr("data-loaded","true");            
            $this.select2({                
                data : jsonResponse
            });
            if($this.attr("amphi-selected-value") != undefined) {
                $(this).val($this.attr("amphi-selected-value").split(",")).trigger("change");
            }            
        } else {            
            $this.select2({});
            if($this.attr("amphi-selected-value") != undefined) {
                $(this).val($this.attr("amphi-selected-value").split(",")).trigger("change");
            }
        }
        $this.on("select2:open", function (e) {
            var alreadyPresent = $(".select2-search__field").parent().find(".close-select2")
            if(alreadyPresent != null && alreadyPresent != undefined){
                alreadyPresent.remove();
            }
            var closeIcon = document.createElement("span");
            closeIcon.className = "pull-left select2-close-action close-select2";
            closeIcon.style["z-index"] = "1";
            closeIcon.style["position"] = "absolute";
            closeIcon.style["right"] = "3%";
            closeIcon.style["top"] = "3%";
            closeIcon.style["cursor"] = "pointer";
            closeIcon.innerHTML = "x";
            $(".select2-search__field").parent().append(closeIcon);   
            closeIcon.addEventListener('click', function (event) {
                $(".select2-search__field").val('');
            });
            var rejectStringArr = ["Search","-Choose Team-","-Select-",""];
            if($("#amphiSearchboxIgnoreDefaultWords").val()){
               var words = $("#amphiSearchboxIgnoreDefaultWords").val().split(",");
               rejectStringArr = rejectStringArr.concat(words);
            }
            var selectedTxt = $this.find(":selected").text();
            if(rejectStringArr.indexOf(selectedTxt) == -1 && $this.attr("multiple") == undefined){  
                $(".select2-search__field").val(selectedTxt);                
            }
        });
    });   
}

function validateEmail() {
    var flag = true;    
    if($("#newUsers").val() != undefined && $("#newUsers").val() != "") {
        var mailIds = $("#newUsers").val().split(",");        
        if(mailIds.length > 0) {
            for (var i = 0; i < mailIds.length; i++) {                
                var mailformat = /^\w+([\.-]?\w+)*@\w+([\.-]?\w+)*(\.\w{2,3})+$/;  
                if(!mailIds[i].match(mailformat)){
                    flag = false
                }        
            }
            if(!flag) {
                alert("Invalid E-Mail Address");
            }
        }
    }
    if(flag) {
        showWait();
    }
    return flag;
}

