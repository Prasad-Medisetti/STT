        $('.doubt-thread').show();
    }
    else if(event.which === 13 && Number(doubtThreadId) > 0){        
        var matchedElement = $(".doubt-thread[data-id='"+doubtThreadId+"']");
        $(".doubt-thread").hide();
        if(matchedElement.length > 0){
            matchedElement.show();
        }else{
            var contextPathURL = contextPath;
            doubtThreadIdSearchRequest = jQuery.ajax({
                method:'post',
                async:false,
                url:contextPathURL+'/doubtThread/obtainDoubtThreadById',
                data:{'doubtThreadId' : doubtThreadId},
                beforeSend : function(){
                    if(doubtThreadIdSearchRequest !== null) {
                        doubtThreadIdSearchRequest.abort();
                    }
                },
                success:function(data){
                    if(JSON.parse(data).length === 0){
                        Snackbar.show({text: 'Doubt thread id not exists', pos : 'top-center',duration : 2000});
                    }else{
                        doubtThreadJsonToHtml(JSON.parse(data),true);                        
                    }
                },
                error:function(e){
                  // Error
                }
            });
        }
    }    
}

