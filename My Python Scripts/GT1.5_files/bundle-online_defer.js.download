        var firstComponentWidth = $firstComponent.width();
        var nlw = leftPercentage;
        if(direction == "left"){
                if(lastComponentWidth > ($splitPane.width()*0.3)){
                        nlw = (($splitPane.width() - (($splitPane.width()/10)+5))/$splitPane.width())*100;
                }
        }
        else{
                if(firstComponentWidth > ($splitPane.width() * 0.3)){
                        nlw = 10;
                }
        }		
        var splitPaneWidth = $splitPane.width(),
        lastComponentMinWidth = 0,
        maxLastComponentWidth = splitPaneWidth  - 5,
        rightOffset = $lastComponent.width() ;
        var right = nlw+"%";
        $firstComponent.css('right', right);
        $divider.css('right', right);
        $lastComponent.css('width', right);
        $splitPane.resize();
}

