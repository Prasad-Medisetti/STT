function setSizeCircuitEditor(windowWidth ,windowHeight, oH){
    windowWidth = windowWidth - 50;
    var getHeighTop = attemptTopSectionHeight();
    var left = document.querySelector('.program-wrapper #left');
    left.style.height = windowHeight - getHeighTop - oH+'px';
    left.style.width = parseFloat(windowWidth / 2 - 10) +'px';
    var right=document.querySelector('.program-wrapper #right');
    right.style.height = windowHeight - getHeighTop - oH+'px';
    right.style.width = parseFloat(windowWidth / 2 - 10) +'px';
    var leftResizer=document.querySelector('.program-wrapper #resizer');
