function secureWindowViolation(){
    secureWindowAjaxTimeout = setTimeout(function(){
        $('#secure-window-warning-modal').modal('show');
        $.ajax({
            url: contextPath + '/amphiSession/secureWindowViolation',
            data:{
                dummy: new Date().getTime()
            },
            success: function(response){
                console.log(response);
                secureWindowViolationCount = response.secureWindowViolationCount;
                if(response.finishAttempt){
                    timeEnd();
                }
            },
            error: function(){
                console.error("Error in secure window check.");
            }
        });
    }, 1000);
}

function cancelSecureWindowViolation(){
    console.log("cancelSecureWindowViolation called");
    clearInterval(secureWindowAjaxTimeout);
}

function loadAdvertisement(url, placeholderCode){
    var status = false;
    $.ajax({
        method:"post",
        url: url,
        async:false,
        success:function(data){
            if(data.indexOf("No Advertisement") === -1 && data.indexOf("Technical Difficulties") === -1){
                status = true;
                $("#ad-placeholder-"+placeholderCode).html(data);
            }else{
                $("#ad-placeholder-"+placeholderCode).remove();
            }
        }
    });
    return status;
}

