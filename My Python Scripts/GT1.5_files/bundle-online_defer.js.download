function slabWinDonut(winRatio, imageURL) {
    var width = 60,
        height = 60,
        radius = Math.min(width, height) / 2;
    var arc = d3.arc()
        .outerRadius(radius - 15)
        .innerRadius(radius - 13);

    $('.time-slot-win-ratio').html("Win Ratio");
    var svg = d3.select(".time-slot-win-ratio").append("svg")
        .attr("width", width)
        .attr("height", height)
        .append("g")
        .attr("transform", "translate(30,18)");

    var colors = [],
        opacityColor = [];
    var aColor = d3.scaleLinear()
        .domain([0,270,360])
        .range(["#FA709A","#FEE140","#FA709A"]);

    d3.range(360).forEach(function(d, i) {
        var start = i * (Math.PI / 180),
            end = (i + 1) * (Math.PI / 180);
        if(i<((winRatio/100)*360)){
            colors.push({
                startAngle: start,
                endAngle: end,
                fill: aColor(i)
            });
        }

        opacityColor.push({
            startAngle: start,
            endAngle: end,
            fill: aColor(i)
        });
    });
    svg.selectAll('.arc')
        .data(colors)
        .enter()
        .append('path')
        .transition().delay(function(d,i) { return i * 5; }).duration(10)
        .attr('class', 'arc')
        .attr('d', arc)
        .style('opacity',1)
        .style('fill', function(d){
            return d.fill;
        })
        .style('stroke',function(d){return d.fill;});


    svg.selectAll('.arc')
        .data(opacityColor)
        .enter()
        .append('path')
        .attr('class', 'arc')
        .attr('d', arc)
        .style('opacity',0.2)
        .style('fill', function(d,i){
            return aColor(i);
        })
        .style('opacity',0.2);

    var middleCount=svg.append('text')
        .text(winRatio)
        .attr('class','middleText')
        .attr('text-anchor','middle')
        .attr('dy',4)
        .attr('dx',-1)
        .style('fill',"#ffffff")
        .style('font-size','9px');

    svg.append('text')
        .text('%')
        .attr('class','percent')
        .attr('text-anchor','middle')
        .attr('dx',function(){if(winRatio==100)return 10;else if(winRatio<10)return 6 ;return 8;})
        .attr('dy',4)
        .style('fill',"#ffffff")
        .style('font-size','7px');

    svg.append("svg:image")
        .attr("xlink:href",imageURL)
        .attr("width",15)
        .attr("height",15)
        .attr("opacity",0.5)
        .attr("x",-1*15/2)
        .attr("y",-1*15/2);
}

