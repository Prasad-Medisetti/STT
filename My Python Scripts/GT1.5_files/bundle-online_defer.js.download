function problemNavFunctionCallShort(w, h,dd){
    problemSideNavigation(w, h, dd);
    window.addEventListener("resize", function(){problemSideNavigation(window.innerWidth, window.innerHeight, dd)});
    document.querySelector('#navbar-toggle').addEventListener("click", function(){ProblemNavbarToggle(this, w, h, dd)});        
}

function scodhvestAttemptJS(){
    if(document.querySelector('#scodvestChoose')){
        document.querySelectorAll('#scodvestChoose .program-indicators li').forEach(function(selList){
            selList.addEventListener('click',function(event){
                event.preventDefault();
                this.parentElement.querySelector('li.active').classList.remove('active');
                this.classList.add('active');
                var dataTarget = this.querySelector('a').getAttribute('href');
                document.querySelectorAll('.choose-option .choose-list').forEach(function(conList){
                    conList.classList.remove('active');
                });
                document.querySelector('.choose-option .choose-list[data-target='+dataTarget.split('#')[1]+']').classList.add('active');
            })
        });
        document.querySelectorAll('#scodvestChoose .choose-option .choose-list').forEach(function(selList){
            selList.addEventListener('click', function(){

                var getSelectedIndicatorEl = document.querySelector('#scodvestChoose .program-indicators li a[href="#'+this.getAttribute('data-target')+'"]').parentElement;
                if(this.parentElement.querySelector('.choose-list.selected')){
                    this.parentElement.querySelector('.choose-list.selected').classList.remove('selected');
                    document.querySelector('#scodvestChoose .program-indicators li.selected').classList.remove('selected');
                    this.parentElement.querySelector('.choose-list.error') && this.parentElement.querySelector('.choose-list.error').classList.remove('error');
                    this.parentElement.querySelector('.choose-list.success') && this.parentElement.querySelector('.choose-list.success').classList.remove('success');
                    document.querySelector('#scodvestChoose .program-indicators li.error') && document.querySelector('#scodvestChoose .program-indicators li.error').classList.remove('error');
                    document.querySelector('#scodvestChoose .program-indicators li.success') && document.querySelector('#scodvestChoose .program-indicators li.success').classList.remove('success');
                    this.classList.add('selected');
                    getSelectedIndicatorEl.classList.add('selected');
                } else {
                    this.classList.add('selected');
                    getSelectedIndicatorEl.classList.add('selected');

                }
            });
        });
    }
}

function myStatus(e, winH, $this){ 
    e.preventDefault();
    var getId = $this.getAttribute('href');
    if($this.closest('li.active')){
        $this.closest('li').classList.remove('active');
        document.querySelector(getId).classList.remove('show');
        document.querySelector(getId).classList.add('hide');
    } else {
        var getContentLoaded = $("#my-status-wrapper").data('content-loaded');
        var getContentURL = $("#my-status-wrapper").data('content-url');
        if(getContentLoaded == 'true'){
            $this.closest('li').classList.add('active');
            document.querySelector(getId).classList.remove('hide');
            document.querySelector(getId).classList.add('show');
        } else {
            $.ajax({         
                url: getContentURL,        
                before: showWait(),
                success: function(data) {
                    $("#showAttemptStatus").html(data);
                    hideWait();
                    $this.closest('li').classList.add('active');
                    document.querySelector(getId).classList.remove('hide');
                    document.querySelector(getId).classList.add('show');
                    var getMyStatusHeight = $("#showAttemptStatus").height();
                    if(getMyStatusHeight > 375){
                        document.getElementById('scroll-bar')?document.getElementById('scroll-bar').style.height = winH - 300+'px':null;
                        $('#scroll-bar').fakeScroll(); 
                    } else {
                        document.getElementById('scroll-bar')?document.getElementById('scroll-bar').style.maxHeight = winH - 300+'px':null;
                    }
                },
                complete:hideWait(),
                error: function(data) {
                    hideWait();
                }
            });            
        }
    }
}

